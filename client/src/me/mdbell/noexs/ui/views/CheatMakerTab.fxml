<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="470.0" prefWidth="1022.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
   fx:controller="me.mdbell.noexs.ui.controllers.CheatMakerController">
   <children>
      <TitledPane animated="false" collapsible="false" layoutX="14.0" layoutY="23.0" prefHeight="317.0" prefWidth="992.0" text="Code">
         <content>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="298.0" prefWidth="990.0">
               <children>
                  <TextArea fx:id="cheatSource" layoutX="14.0" layoutY="14.0" prefHeight="247.0" prefWidth="474.0" />
                  <TextArea fx:id="cheatCode" layoutX="502.0" layoutY="14.0" prefHeight="247.0" prefWidth="474.0" />
                  <Button layoutX="438.0" layoutY="264.0" mnemonicParsing="false" onAction="#onMakeCheat" prefHeight="25.0" prefWidth="49.0" text="Make" />
                  <Button layoutX="102.0" layoutY="264.0" mnemonicParsing="false" onAction="#onSaveSource" text="Save" />
                  <Button layoutX="595.0" layoutY="264.0" mnemonicParsing="false" onAction="#onSaveCode" text="Save to file" />
                  <Button layoutX="155.0" layoutY="264.0" mnemonicParsing="false" onAction="#onLoadSource" text="Load" />
                  <Label layoutX="15.0" layoutY="267.0" prefHeight="17.0" prefWidth="57.0" text="Source :" />
                  <Label layoutX="503.0" layoutY="267.0" text="Cheat Code :" />
                  <Button layoutX="274.0" layoutY="264.0" mnemonicParsing="false" onAction="#onGetBookmark" text="Book" />
               </children>
            </AnchorPane>
         </content>
      </TitledPane>
      <TitledPane animated="false" collapsible="false" layoutX="14.0" layoutY="353.0" prefHeight="102.0" prefWidth="992.0" text="Console">
         <content>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
               <children>
                  <TextArea fx:id="console" editable="false" layoutX="1.0" layoutY="1.0" prefHeight="73.0" prefWidth="990.0" />
               </children>
            </AnchorPane>
         </content>
      </TitledPane>
   </children>
</AnchorPane>
